
n = int(input())
a = 0
for b in range(n):
    i = int(input())
    if i == 0:
        a += 1
print(a)


a = int(input())
b = 1
for i in range (1, a):
    if a % i == 0:
        b += 1;
print(b)




a, b = map(int, input().split())  
for i in range (a, b + 1, 1):
    if i % 2 == 0:
        print(i, end=' ');
print()
